<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="gowfinmfbsoftwares.com">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/logo"
            android:label="GowMobile"
            android:roundIcon="@drawable/logo"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".GroupActivity"
                android:label="@string/title_activity_group"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".AllClientActivity">
        </activity>
        <activity android:name=".searchActivity">
        </activity>
        <activity android:name=".searchloanActivity">
        </activity>
        <activity android:name=".addnewActivity">
        </activity>
        <activity android:name=".depositActivity">
        </activity>
        <activity
                android:name=".optionActivity"
                android:label="Control Panel"
                android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>